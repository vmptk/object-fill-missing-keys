{
  "name": "object-fill-missing-keys",
  "version": "2.1.3",
  "description": "Add missing keys into plain objects, according to a provided schema object.",
  "main": "index.js",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "precommit": "npm test",
    "test": "clear && standard && nyc --reporter=html --reporter=text ava",
    "watch": "nodemon --quiet --watch . --exec npm run test"
  },
  "repository": "codsen/object-fill-missing-keys",
  "keywords": [
    "ast",
    "json",
    "plain",
    "object",
    "posthtml-tree",
    "posthtmltree",
    "add",
    "missing",
    "fill",
    "keys",
    "schema",
    "unify",
    "normalise",
    "normalize"
  ],
  "author": {
    "name": "Roy Revelt",
    "email": "roy@codsen.com",
    "url": "codsen.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codsen/object-fill-missing-keys/issues"
  },
  "homepage": "https://github.com/codsen/object-fill-missing-keys#readme",
  "dependencies": {
    "lodash.clonedeep": "*",
    "object-merge-advanced": "^7.0.2",
    "type-detect": "^4.0.3"
  },
  "devDependencies": {
    "ava": "*",
    "clear-cli": "*",
    "coveralls": "*",
    "husky": "*",
    "nyc": "*",
    "standard": "^10.0.3"
  },
  "ava": {
    "verbose": true
  }
}
